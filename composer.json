{
  "name": "brezgalov/yii2-external-api-logger",
  "description": "logger for api requests",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Brezgalov",
      "email": "oleg.brezgalov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "yiisoft/yii2": ">=2.0.6",
    "ext-json": "*",
    "brezgalov/php-typed-collection": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit test",
    "test-coverage": "./vendor/bin/phpunit test --coverage-html coverage",
    "test-v2": "./vendor/bin/phpunit ./v2/test/ -c ./v2/phpunit.xml",
    "test-v2-coverage": "./vendor/bin/phpunit ./v2/test/ -c ./v2/phpunit.xml --coverage-html ./v2/coverage"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brezgalov\\ExtApiLogger\\": "src/",
      "Brezgalov\\ExtApiLogger\\Tests\\": "test/",
      "Brezgalov\\ExtApiLogger\\v2\\": "v2/src/",
      "Brezgalov\\ExtApiLogger\\v2\\Tests\\": "v2/test/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
